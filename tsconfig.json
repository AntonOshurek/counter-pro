{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "skipLibCheck": true,
    "noFallthroughCasesInSwitch": true,
    "strictNullChecks": true,
    "paths": {
      "@features/*": [
        "src/features/*"
      ],
      "@entities/*": [
        "src/entities/*"
      ],
      "@shared/*": [
        "src/shared/*"
      ],
      "@ui/*": [
        "src/shared/ui/*"
      ],
      "@pages/*": [
        "src/pages/*"
      ],
      "@widgets/*": [
        "src/widgets/*"
      ],
      "@service/*": [
        "src/services/*"
      ]
    }
  }
}
